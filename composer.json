{
  "name": "dotkernel/dot-ems",
  "type": "library",
  "description": "Entity-mapper-service abstraction layer",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-servicemanager": "^3.0",

    "dotkernel/dot-helpers": "0.6.x-dev",
    "dotkernel/dot-event": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-db": "^2.8",
    "zendframework/zend-validator": "^2.8",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-paginator": "^2.7"
  },
  "suggest": {
    "zendframework/zend-db": "Needed if you use the db mappers",
    "zendframework/zend-validator": "Needed if you use the provided custom validators",
    "zendframework/zend-hydrator": "Needed by mappers to return hydrated entity objects",
    "zendframework/zend-paginator": "Needed for retrieving paginated result sets"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Ems\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Ems\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
